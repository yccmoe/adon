<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
    <Frame name="TomCats_Config" parent="InterfaceOptionsFramePanelContainer">
        <Layers>
            <Layer>
                <Texture file="Interface\AddOns\TomCats\images\tomcat-logo.blp">
                    <Size x="128" y="128" />
                    <Anchors>
                        <Anchor point="TOPLEFT" x="16" y="-16" />
                    </Anchors>
                </Texture>
            </Layer>
            <Layer>
                <FontString name="$parentTitle" text="TomCat's Tours" inherits="GameFontNormalLarge">
                    <Anchors>
                        <Anchor point="TOPLEFT" x="160" y="-16" />
                    </Anchors>
                </FontString>
                <FontString name="$parentVersionLabel" text="Version" inherits="GameFontNormal">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="$parentTitle" relativePoint="BOTTOMLEFT" y="-16" />
                    </Anchors>
                </FontString>
                <FontString name="$parentVersion" text="1.4.5" inherits="GameFontHighlight">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="$parentVersionLabel" relativePoint="TOPRIGHT" x="4" />
                    </Anchors>
                </FontString>
                <FontString name="$parentReleaseDateLabel" text="Released" inherits="GameFontNormal">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="$parentVersionLabel" relativePoint="BOTTOMLEFT" y="-6" />
                    </Anchors>
                </FontString>
                <FontString name="$parentVersion" text="18-Jan-2020" inherits="GameFontHighlight">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="$parentReleaseDateLabel" relativePoint="TOPRIGHT" x="4" />
                    </Anchors>
                </FontString>
                <FontString name="$parentAuthorLabel" text="Author" inherits="GameFontNormal">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="$parentReleaseDateLabel" relativePoint="BOTTOMLEFT" y="-6" />
                    </Anchors>
                </FontString>
                <FontString name="$parentAuthor" text="TomCat" inherits="GameFontHighlight">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="$parentAuthorLabel" relativePoint="TOPRIGHT" x="4" />
                    </Anchors>
                </FontString>
                <FontString parentKey="InstalledComponents" inherits="GameFontNormal" justifyH="LEFT">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="$parentAuthorLabel" relativePoint="BOTTOMLEFT" y="-6" />
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
    </Frame>
    <Frame name="TomCats_Config_Slash_Commands" parent="InterfaceOptionsFramePanelContainer">
        <Frames>
            <SimpleHTML parentKey="html">
                <Size x="591" y="536"/>
                <Anchors>
                    <Anchor point="TOPLEFT" x="16" y="-16" />
                    <Anchor point="BOTTOMRIGHT" x="-16" y="16" />
                </Anchors>
                <FontString inherits="GameFontHighlight" justifyH="LEFT" spacing="1" />
                <FontStringHeader1 inherits="GameFontNormalLarge" justifyH="LEFT" spacing="1"/>
                <FontStringHeader2 inherits="GameFontNormalMed2" justifyH="LEFT" spacing="1" />
                <FontStringHeader3 inherits="GameFontNormal" justifyH="LEFT" spacing="1" />
            </SimpleHTML>
        </Frames>
    </Frame>
</Ui>
